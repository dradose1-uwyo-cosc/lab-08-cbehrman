with open('prompt.txt', 'r') as prompt, open('out.txt', 'w') as out:
    for line in prompt:
        output = " "
        pairs= line.split ("\t")
        for pair in pairs:
            if ":" in pair:
                key, value =pair.split(":")
                value=int(value)
                if key == "*":
                    output += "* "*value
                elif key == "w":
                    output += "  "*value
        out.write(output + "\n")
        
        